
<div id="showrun-list">

<table class="horizontal_table" style="width: 100%; height: 10px;" border="1" cellspacing="0">
<tbody>
 
    <tr valign="top" class="<%= cycle('list-line-odd', 'list-line-even') %>" >
    <td width="20%" ><%= showrun.player %></td>
    <td width="15%"><%= showrun.event %></td>
    <td width="10%"><%= showrun.spec_vote %></td>
    <td width="15%"><%= showrun.level %></td>
    <td width="10%"><%= showrun.make_other %></td>
    <td width="20%"><%= showrun.note_showrun %></td>
    <td width="5%" ><%= link_to 'Edit', edit_showblog_showrun_path(showrun.showblog, showrun)  %></td>
    <td width="5%" ><%= link_to 'Remove', [@showblog, showrun], confirm: 'Are you sure?', method: :delete %></td>

</tr>
</tbody>
</table>
</div>
