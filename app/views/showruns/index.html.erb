<h1>Listing showruns</h1>
<table class="horizontal_table" style="width: 100%; height: 10px;" border="1">
<tbody>

  <tr>
    <th width="25%">Player </th>
    <th width="15%">Event</th>
    <th width="10%">Vote</th>
    <th width="15%">Level</th>
   <th width="10%">Make Other ?</th>
   <th width="25%">Note</th>
  </tr>
</tbody>
</table>

<table class="horizontal_table" style="width: 100%; height: 10px;" border="1" cellspacing="0">
<tbody>
    <tr>
    <td width="25%" ><%= showrun.player %></td>
    <td width="15%"><%= showrun.event %></td>
    <td width="10%"><%= showrun.spec_vote %></td>
    <td width="15%"><%= showrun.level %></td>
    <td width="10%"><%= showrun.make_other %></td>
    <td width="25%"><%= showrun.note_showrun %></td>
  </tr>
</tbody>
<% end %>
</table>

