<div id="showrun-list">

<table class="horizontal_table" style="width: 100%; height: 10px;" border="1" cellspacing="0">
<tbody>

    <tr valign="top" class="<%= cycle('list-line-odd', 'list-line-even') %>" >
    <td width="15%" ><%= horselist.player %></td>
    <td width="15%"><%= horselist.horse %></td>
    <td width="15%"><%= horselist.currant_level %></td>
    <td width="15%"><%= horselist.range %></td>
    <td width="15%"><%= horselist.date %></td>
    <td width="15%"><%= horselist.note %></td>
    <td width="5%" ><%= link_to 'Edit', edit_request_horselist_path(horselist.request, horselist)  %></td>
    <td width="5%" ><%= link_to 'Remove', [@request, horselist], confirm: 'Are you sure?', method: :delete %></td>

</tr>
</tbody>
</table>
</div>
